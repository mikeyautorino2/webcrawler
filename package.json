    "build-client": "cd client && npm run build",
    "build": "./build.sh",
    "vercel-build": "./build.sh",
    "dev-full": "concurrently \"npm run dev\" \"npm run client\"",
    "test": "jest --detectOpenHandles"
  },
  "keywords": [
    "webcrawler",
    "link-analyzer",
    "express",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  }
