{
  "name": "link-analyzer",
  "version": "1.0.0",
  "description": "Web crawler and link analyzer API",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "client": "cd client && npm start",
